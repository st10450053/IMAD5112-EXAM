<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PAGE2">

    <ImageView
        android:id="@+id/LOGO2imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/weather_forecast_logo_file_free_vector"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="38dp" />

    <Button
        android:id="@+id/CLEARbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CLEAR"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="611dp" />

    <Button
        android:id="@+id/NEXTbutton2"
        android:layout_width="119dp"
        android:layout_height="52dp"
        android:text="VIEW DETAILS"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="607dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="285dp" />

    <Button
        android:id="@+id/ADDbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD"
        tools:layout_editor_absoluteX="147dp"
        tools:layout_editor_absoluteY="611dp" />

    <EditText
        android:id="@+id/MineditText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Minimum temp:"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="259dp" />

    <EditText
        android:id="@+id/MaxeditText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Maximum temp:"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="331dp" />

    <EditText
        android:id="@+id/DATEeditText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="Date:"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="190dp" />

    <EditText
        android:id="@+id/NOTESeditText4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="text"
        android:text="ACTIVITY NOTES:"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="398dp" />

</androidx.constraintlayout.widget.ConstraintLayout>